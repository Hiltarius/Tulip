import sys

def regulon(entry_file):
    
    locustoid = {}
    file = open(entry_file,'r')
    for line in file:
        split = line.split('\t')
        if split[2] != None:
            try:
                locustoid[split[0]]=split[2] 
            except:
                locustoid[split[0]]=split[2]
                
    empty_keys = [k for k,v in locustoid.iteritems() if not v]
    for k in empty_keys:
        del locustoid[k]

        
    return locustoid

   
            
reg = regulon("ecoliregulon.txt")
convert("clust2.txt",reg,"test")

    
    
    
